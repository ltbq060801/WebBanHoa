@model IEnumerable<WebQuanLyBanHoa.Models.LoaiThanhVien>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}

<h2>Loại thành viên</h2>

<table class="table">
    <tr>
        <th>
            Mã loại TV
        </th>
        <th>
            Tên loại
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaLoaiTV)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenLoai)
            </td>
            <td>
                @Html.ActionLink("Phân Quyền", "PhanQuyen", new { id = item.MaLoaiTV })
            </td>
        </tr>
    }

</table>
